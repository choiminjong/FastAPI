
CREATE TABLE `users` (
    `no` int NOT NULL AUTO_INCREMENT,
    `status` char(15) DEFAULT NULL,
    `email` char(255) DEFAULT NULL,
    `pw` text(2000) DEFAULT NULL,
    `name` char(255) DEFAULT NULL,
	`phone_number` char(20) DEFAULT NULL,
	`profile_img` text(1000) DEFAULT NULL,
	`sns_type` char(5) DEFAULT NULL,
	`marketing_agree` char(1) DEFAULT NULL,
    PRIMARY KEY (`no`)
);